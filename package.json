{
  "name": "@racg211/ui-library",
  "version": "1.0.1",
  "description": "Minimal UI component library for React using Tailwind or styled-components",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
