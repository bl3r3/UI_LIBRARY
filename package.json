  "repository": {
    "type": "git",
    "url": "https://github.com/bl3r3/ui-library"
  },
  "author": "Roberto Carrasquel <carrasquel18@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "ui",
    "component-library",
    "tailwind",
    "styled-components"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.34",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
